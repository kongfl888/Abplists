<?xml version="1.0" encoding="UTF-8"?>
<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
    <ShortName>m百度(HTTPS)</ShortName>
    <Description>百度网页搜索(HTTPS)-移动版</Description>
    <InputEncoding>UTF-8</InputEncoding>
    <Image width="16" height="16">data:image/x-icon;base64,</Image>
    <moz:SearchForm>https://m.baidu.com/</moz:SearchForm>
    <Url type="text/html" method="GET" template="https://m.baidu.com/s">
        <Param name="word" value="{searchTerms}"/>
    </Url>
    <Url type="application/x-suggestions+json" method="GET" template="https://m.baidu.com/su">
        <Param name="wd" value="{searchTerms}"/>
        <Param name="action" value="opensearch"/>
        <Param name="ie" value="UTF-8"/>
        <Param name="from" value="iphone"/>
    </Url>
</OpenSearchDescription>
